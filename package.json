{
    "name": "QuikFeedback",
    "version": "1.0.0",
    "description": "A WebApp that collects end-user feedback for buisnesses and companies",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "server": "nodemon index.js",
        "client": "npm run start --prefix client",
        "dev": "concurrently \"npm run start\" \"npm run client\"",
        "aws-postbuild":"NPM_CONFIG_PRODUCTION=false npm install --prefix client  && npm run build --prefix client"
    },
    "engines": {
        "node": "8.9.1",
        "npm": "5.5.1"
    },
    "author": "Jaymit Desai",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.151.0",
        "body-parser": "^1.18.2",
        "cookie-session": "^2.0.0-beta.3",
        "express": "^4.16.2",
        "materialize-css": "^0.100.2",
        "nodemon": "^1.12.1",
        "passport": "^0.4.0",
        "passport-google-oauth20": "^1.0.0",
        "stripe": "^5.3.0",
        "uuid": "^3.1.0"
    },
    "devDependencies": {
        "concurrently": "^3.5.0"
    }
}
